{
    "name": "bitbybit-node-core",
    "version": "1.0.0",
    "description": "Example of how to use bitbybit-occt within express app",
    "main": "index.js",
    "scripts": {
        "build": "npx tsc -p tsconfig.json",
        "start": "node --import=extensionless/register dist/index.js",
        "dev": "concurrently \"npx tsc --watch\" \"nodemon --import=extensionless/register -q dist/index.js\""
    },
    "author": "Bit By Bit Developers",
    "license": "MIT",
    "dependencies": {
        "@bitbybit-dev/core": "0.20.8",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "opentype.js": "1.3.4",
        "xhr2": "0.2.1"
    },
    "type": "module",
    "devDependencies": {
        "extensionless": "1.9.9",
        "@types/express": "^4.17.16",
        "@types/node": "^18.11.18",
        "concurrently": "^7.6.0",
        "nodemon": "^2.0.20",
        "typescript": "^4.9.4"
    },
    "extensionless": {
        "lookFor": [
            "js",
            "mjs",
            "cjs"
        ]
    },
    "engines": {
        "node": ">=20.19.4"
    }
}