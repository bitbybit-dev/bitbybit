{
    "name": "basic",
    "version": "1.0.0",
    "description": "Example of how to use @bitbybit-dev/occt within node",
    "main": "index.js",
    "scripts": {
        "build": "npx tsc",
        "start": "node --import=extensionless/register dist/index.js",
        "dev": "concurrently \"npx tsc --watch\" \"nodemon --import=extensionless/register -q dist/index.js\""
    },
    "author": "Bit By Bit Developers",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=20.19.4"
    },
    "dependencies": {
        "@bitbybit-dev/occt": "^0.20.9"
    },
    "devDependencies": {
        "extensionless": "1.9.9",
        "concurrently": "^7.6.0",
        "nodemon": "^2.0.20",
        "typescript": "^4.9.4"
    },
    "extensionless": {
        "lookFor": [
            "js",
            "mjs",
            "cjs"
        ]
    }
}